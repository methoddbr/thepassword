<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="centralPane" prefHeight="318.0" prefWidth="448.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="password.InitialController">
   <children>
      <AnchorPane prefHeight="318.0" prefWidth="449.0">
         <children>
            <Button fx:id="versus" layoutX="180.0" layoutY="219.0" mnemonicParsing="false" onAction="#clickButton" prefHeight="25.0" prefWidth="79.0" text="Two Players">
               <tooltip>
                  <Tooltip text="One player prepares a password for another to guess." />
               </tooltip></Button>
            <Button fx:id="only" layoutX="33.0" layoutY="219.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="79.0" text="One player">
               <tooltip>
                  <Tooltip text="Randon password." />
               </tooltip></Button>
            <Button fx:id="inst" layoutX="180.0" layoutY="159.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="79.0" text="Instructions">
               <tooltip>
                  <Tooltip text="Instructions of the game." />
               </tooltip></Button>
            <Label fx:id="logo" layoutX="51.0" layoutY="65.0" onMouseClicked="#clickLogo" text="The Password">
               <font>
                  <Font name="OCR A Extended" size="48.0" />
               </font>
            </Label>
            <Button fx:id="record" layoutX="329.0" layoutY="219.0" mnemonicParsing="false" onAction="#clickButton" prefHeight="25.0" prefWidth="79.0" text="Records" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
